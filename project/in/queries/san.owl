        <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:comment>Specification of the &quot;acts on&quot; property, where the entity acted upon is a Property. It is a usual case for actuators, such as lamps (acting on luminosity) or heaters (acting on temperature)</rdfs:comment>
        <rdfs:label>acts on property</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#isConsumedBy -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#isConsumedBy">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:comment>Describes a relation between a consumer and a consumee</rdfs:comment>
        <rdfs:label>is consumed by</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#isController -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#isController">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:comment>An entity is controller of another one when it has influence on its decision process or evolution in time.</rdfs:comment>
        <rdfs:label>is controller</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#receivedActuation -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#receivedActuation">
