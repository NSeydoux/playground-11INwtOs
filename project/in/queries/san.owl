    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actsOn -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actsOn">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#isActedUponBy"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:comment>Relationship between an entity that acts upon a quantity or another entity</rdfs:comment>
        <rdfs:label>acts on</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actsOnProperty -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actsOnProperty">
