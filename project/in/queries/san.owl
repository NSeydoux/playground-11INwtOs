        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actsOn"/>
                <someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/ActuatableProperty"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#hasActuatingCapability"/>
                <someValuesFrom rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingCapability"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
