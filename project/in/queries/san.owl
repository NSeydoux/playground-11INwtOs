                <someValuesFrom rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Effect"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.w3.org/ns/ssn/implements"/>
