    </Class>
    


    <!-- http://purl.org/vocommons/voaf#Vocabulary -->

    <Class rdf:about="http://purl.org/vocommons/voaf#Vocabulary"/>
    


    <!-- http://qudt.org/schema/qudt#QuantityKind -->

    <Class rdf:about="http://qudt.org/schema/qudt#QuantityKind">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
    </Class>
    


    <!-- http://qudt.org/schema/qudt#Unit -->

    <Class rdf:about="http://qudt.org/schema/qudt#Unit">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#UnitOfMeasure"/>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Acting -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Acting">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Procedure"/>
        <rdfs:comment>Acting is the process that results in an action on the physical world, might it be a motion, a display, the emission of a sound...</rdfs:comment>
        <rdfs:label>Acting</rdfs:label>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingCapability -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingCapability">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#hasActuatingProperty"/>
                <someValuesFrom rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingProperty"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.w3.org/ns/ssn/forProperty"/>
                <someValuesFrom rdf:resource="http://qudt.org/schema/qudt#QuantityKind"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Represents actuation properties such as precision, latency, power consumption, and the environmental conditions in which these actuation can take place normally.</rdfs:comment>
        <rdfs:label>Actuating Capability</rdfs:label>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingDevice -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingDevice">
        <rdfs:subClassOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Actuator"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment>A device that implements actuating</rdfs:comment>
        <rdfs:label>Actuating device</rdfs:label>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingProperty -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingProperty">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:comment>A characteristic of an Actuator that gives information about the Actuations it is able to perform.</rdfs:comment>
        <rdfs:label>Actuating Property</rdfs:label>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingRange -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingRange">
        <rdfs:subClassOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingProperty"/>
        <rdfs:comment>Range of commands that the actuator can receive and transduce into the physical world without damage to itself or security issues to its surrounding environment.</rdfs:comment>
        <rdfs:label>Actuating Range</rdfs:label>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingTime -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingTime">
        <rdfs:subClassOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#ActuatingProperty"/>
        <rdfs:comment>Time between the moment when the actuator receives an actuation command and the actual actuation.</rdfs:comment>
        <rdfs:label>Actuating Time</rdfs:label>
    </Class>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Actuation -->

    <Class rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Actuation">
        <rdfs:subClassOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Situation"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#wasActuationSent"/>
                <someValuesFrom rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Actuator"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesEvent"/>
                <someValuesFrom rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#Effect"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actuationMethodUsed"/>
