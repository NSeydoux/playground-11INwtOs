        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/sosa/actsOnProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:comment>Specification of the &quot;acts on&quot; property, where the entity acted upon is a Property. It is a usual case for actuators, such as lamps (acting on luminosity) or heaters (acting on temperature)</rdfs:comment>
        <rdfs:label>acts on property</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actuationMethodUsed -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN/sosa#actuationMethodUsed">
