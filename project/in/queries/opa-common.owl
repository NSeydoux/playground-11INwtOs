    <!-- https://w3id.org/laas-iot/adream#ADREAMDevice -->

    <owl:Class rdf:about="https://w3id.org/laas-iot/adream#ADREAMDevice">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isComponentOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/laas-iot/adream#ADREAMSubSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Device part of the ADREAM building.</rdfs:comment>
        <rdfs:label>ADREAM device</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/laas-iot/adream#ADREAMLightSensor -->

    <owl:Class rdf:about="https://w3id.org/laas-iot/adream#ADREAMLightSensor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/laas-iot/adream#ADREAMSensor"/>
        <rdfs:label>ADREAM light sensor</rdfs:label>
    </owl:Class>
    


