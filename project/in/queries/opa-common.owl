    <!-- https://w3id.org/laas-iot/adream#ADREAMSensor -->

    <owl:Class rdf:about="https://w3id.org/laas-iot/adream#ADREAMSensor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/laas-iot/adream#ADREAMDevice"/>
        <rdfs:comment>A sensor inside the ADREAM building. Each sensor is relevant to one of the ADREAM subsystems : energy, light, temperature or power.</rdfs:comment>
        <rdfs:label>ADREAM sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/laas-iot/adream#ADREAMSubSystem -->

    <owl:Class rdf:about="https://w3id.org/laas-iot/adream#ADREAMSubSystem">
