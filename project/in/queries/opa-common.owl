        <rdfs:comment>A sensor that measure the power generated by a generator (e.g. solar panel)</rdfs:comment>
        <rdfs:label>ADREAM power creation sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/laas-iot/adream#ADREAMPowerSensor -->

    <owl:Class rdf:about="https://w3id.org/laas-iot/adream#ADREAMPowerSensor">
