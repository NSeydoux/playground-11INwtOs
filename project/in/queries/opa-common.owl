    <owl:Ontology rdf:about="https://w3id.org/laas-iot/adream">
        <owl:imports rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O"/>
        <rdfs:comment xml:lang="en">This knowledge base describes the ADREAM building, with a particular focus on the connected devices. It is an extension of IoT-O (https://www.irit.fr/recherches/MELODI/ontologies/IoT-O), an core domain IoT ontology.

ADREAM is a positive-energy building, built for research purposes in Toulouse at LAAS-CNRS. It hosts over 6500 sensors scattered all over its surface to monitor not only energy consumption, but also luminosity or temperature, allowing autonomic control of the building.</rdfs:comment>
        <vann:preferredNamespaceURI>https://w3id.org/laas-iot/adream</vann:preferredNamespaceURI>
        <dc:title>OPA ontology</dc:title>
        <terms:modified>2017-05-03</terms:modified>
        <cc:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
        <dc:creator rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/contributors#hernandez"/>
        <dc:publisher>LAAS-CNRS (https://www.laas.fr)</dc:publisher>
        <voaf:specializes rdf:resource="https://www.irit.fr/recherches/MELODI/ontologies/IoT-O"/>
        <terms:issued>2016-02-10</terms:issued>
        <dc:creator>Nicolas Seydoux, Khalil Drira, Christelle Ecrepont, Nathalie Hernandez, Thierry Monteil</dc:creator>
        <dc:creator rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/contributors#drira"/>
        <dc:creator rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/contributors#ecrepont"/>
        <vann:preferredNamespacePrefix>opa</vann:preferredNamespacePrefix>
        <dc:publisher>IRIT (https://www.irit.fr/)</dc:publisher>
        <versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.3.4</versionInfo>
        <dc:creator rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/contributors#monteil"/>
        <dc:creator rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/contributors#seydoux"/>
    </owl:Ontology>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->    


    <!-- https://w3id.org/laas-iot/adream#belongsToADREAMSubsystem -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#belongsToADREAMSubsystem">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isComponentOf"/>
        <rdfs:domain rdf:resource="https://www.irit.fr/recherches/MELODI/ontologies/IoT-O#IoT_Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/laas-iot/adream#ADREAMSubSystem"/>
        <rdfs:label>belongs to ADREAM subsystem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/laas-iot/adream#can_be_accessed_from -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#can_be_accessed_from">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
        <rdfs:comment xml:lang="en">Marks the relationship between two places, when one can be accessed from the other. In the majority of cases, the relationship is symmetric.</rdfs:comment>
        <rdfs:label xml:lang="en">can be accessed from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/laas-iot/adream#hasMeasurementUnit -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#hasMeasurementUnit">
        <rdfs:label xml:lang="en">has measurement unit</rdfs:label>
        <rdfs:comment xml:lang="en">Specifies a kind of capability a sensor can have, namely the unit in which it expresses its observations.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/laas-iot/adream#hasIndoorLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#hasIndoorLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation"/>
        <rdfs:range rdf:resource="http://elite.polito.it/ontologies/dogont.owl#BuildingEnvironment"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
            <rdf:Description rdf:about="https://w3id.org/laas-iot/adream#hasIndoorLocation"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">has indoor location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/laas-iot/adream#locatedInBuilding -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#locatedInBuilding">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/laas-iot/adream#hasIndoorLocation"/>
        <rdfs:range rdf:resource="http://elite.polito.it/ontologies/dogont.owl#Building"/>
        <rdfs:label xml:lang="en">located in building</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/laas-iot/adream#locatedInRoom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#locatedInRoom">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/laas-iot/adream#hasIndoorLocation"/>
        <rdfs:range rdf:resource="http://elite.polito.it/ontologies/dogont.owl#Room"/>
        <rdfs:label xml:lang="en">located in room</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/laas-iot/adream#locatedOnFloor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/laas-iot/adream#locatedOnFloor">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/laas-iot/adream#hasIndoorLocation"/>
        <rdfs:range rdf:resource="http://elite.polito.it/ontologies/dogont.owl#Floor"/>
        <rdfs:comment xml:lang="en">Relationship between an entity and the building floor where it can be found</rdfs:comment>
        <rdfs:label xml:lang="en">located on floor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/laas-iot/adream#hasLocationName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/laas-iot/adream#hasLocationName">
        <rdfs:domain rdf:resource="https://www.irit.fr/recherches/MELODI/ontologies/IoT-O#IoT_Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the location of a thing</rdfs:comment>
        <rdfs:label>has location name</rdfs:label>
    </owl:DatatypeProperty>
    


